{
    "compilerOptions": {
        "outDir": "./dist/",
        "strictNullChecks": true,
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true
    },
    "include": ["./src/"],
    "exclude": ["./node_modules/"]
}
